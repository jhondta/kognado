<div id="<%= dom_id country_timezone %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Country:</strong>
    <%= country_timezone.country_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Timezones:</strong>
    <%= country_timezone.timezones_id %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Is primary:</strong>
    <%= country_timezone.is_primary %>
  </p>

</div>
